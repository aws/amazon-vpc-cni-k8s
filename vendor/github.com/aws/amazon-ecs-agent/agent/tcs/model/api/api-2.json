      "input":{"shape":"StartTelemetrySessionRequest"},
      "output":{"shape":"StopTelemetrySessionMessage"},
      "errors":[
        {
          "shape":"BadRequestException",
          "exception":true
        },
        {
          "shape":"ResourceValidationException",
          "exception":true
        },
        {
          "shape":"ServerException",
          "exception":true
        }
      ]
