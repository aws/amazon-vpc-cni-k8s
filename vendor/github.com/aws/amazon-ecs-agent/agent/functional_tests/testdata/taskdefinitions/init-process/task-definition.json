{
  "family": "ecsftest-init-process",
  "containerDefinitions": [{
    "image": "127.0.0.1:51670/ubuntu:latest",
    "name": "exit",
    "cpu": 10,
    "memory": 10,
    "linuxParameters": {
      "initProcessEnabled":true
    },
    "command": ["sh", "-c", "if pidof init == 1; then exit 42; else exit 1; fi"]
  }]
}
